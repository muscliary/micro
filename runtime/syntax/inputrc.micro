syntax "inputrc" "inputrc$"

color constant.bool.false   "\b(off|none)\b"
color constant.bool.true "\bon\b"
color preproc   "\bset|\$include\b"
color constant.string ""(\\.|[^"])*"|'(\\.|[^'])*'"
color constant.specialChar     "\\.?"
color comment "(^|[[:space:]])#([^{].*)?$"
color indent-char.whitespace "[[:space:]]+$"
color indent-char "	+ +| +	+"
